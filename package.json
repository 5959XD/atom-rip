{
  "name": "atom-rip",
  "main": "./lib/atom-rip",
  "version": "1.0.1",
  "description": "당신의 코딩에 휴식을!",
  "keywords": [
    "rip",
    "relaxing",
    "health",
    "rest"
  ],
  "repository": "https://github.com/5959XD/atom-rip",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "typeface-nanum-square": "^1.1.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "configSchema": {
    "codingTime": {
      "title": "코딩 시간",
      "description": "이 시간만큼 일하면 쉽니다. 단위는 **초**입니다.",
      "type": "integer",
      "default": "120"
    },
    "restTime": {
      "title": "휴식 시간",
      "description": "이 시간만큼 쉬면 다시 일합니다. 단위는 **초**입니다.",
      "type": "integer",
      "default": "30"
    }
  }
}
